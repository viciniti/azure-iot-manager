{
  "name": "azure-iot-manager",
  "version": "0.0.5",
  "description": "azure api wrapper, helping to create resources required to use Azure IoT",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "tslint -p tsconfig.json",
    "watch:build": "tsc --watch",
    "prepublishOnly": "npm i && tsc --declaration && npm test && npm run lint"
  },
  "keywords": [
    "azure",
    "iot",
    "typescript",
    "sdk",
    "dps",
    "iot hub",
    "node",
    "arm"
  ],
  "author": "armanpiloyan",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/faker": "^4.1.12",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "faker": "^4.1.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "qs": "^6.9.4",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/node": "^14.0.13",
    "@types/qs": "^6.9.3",
    "axios": "^0.19.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viciniti/azure-iot-manager.git"
  },
  "bugs": {
    "url": "https://github.com/viciniti/azure-iot-manager/issues"
  },
  "homepage": "https://github.com/viciniti/azure-iot-manager#readme"
}
